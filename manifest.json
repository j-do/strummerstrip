{
    "name": "Strummer Strip",
    "description": "All the modal, scales, with the corresponding major minor and diminished chords",
    "manifest_version": 2,
    "minimum_chrome_version": "23",
    "version": "0.1",
    "icons": {
        "16": "icon_16.png",
        "48": "icon_48.png",
        "128": "icon_128.png"
    },
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "app": {
        "background": {
            "scripts": ["background.js"]
        }
    }
}
